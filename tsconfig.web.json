{
  "extends": "@electron-toolkit/tsconfig/tsconfig.web.json",
  "include": [
    "src/web/env.d.ts",
    "src/web/**/*",
    "src/web-preload/*.d.ts",
    "src/shared/**/*"
  ],
  "compilerOptions": {
    "composite": true,
    "jsx": "react-jsx",
    "baseUrl": ".",
    "paths": {
      "@renderer/*": ["src/web/src/*"],
      "@shared/*": ["src/shared/*"]
    }
  }
}
